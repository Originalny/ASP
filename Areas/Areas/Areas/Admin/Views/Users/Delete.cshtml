@model Microsoft.AspNetCore.Identity.IdentityUser
@{
    ViewData["Title"] = "Удалить пользователя";
}
<h4>Удалить пользователя</h4>
<div class="alert alert-warning">Удалить <strong>@Model.Email</strong>?</div>
<form method="post">
    <button class="btn btn-danger">Удалить</button>
    <a asp-action="Index" class="btn btn-secondary">Отмена</a>
</form>
